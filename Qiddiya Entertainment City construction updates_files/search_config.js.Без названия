jQuery(function(){$('#navbar_search').attr('action','/quicksearch.php');var options={script:'/quicksearch-suggest.php?',varname:"query",json:true,shownoresults:true,noresults:"No Results",maxresults:10,cache:false,minchars:1,delay:1,submitform:false,timeout:100000,callback:function(obj){$('#quicksearch_id').val(obj.id);$('#quicksearch_type').val(obj.type);window.location.href=obj.link;return false;}};var as_json=new bsn.AutoSuggest('query',options);});